package view;
import javax.swing.JPanel;
import java.awt.BorderLayout;
import javax.swing.JLabel;
import java.awt.GridLayout;
import javax.swing.JTextField;
import javax.swing.JButton;
import javax.swing.JTable;

public class TelaArma extends Tela {
	private JPanel panelCampos;
	private JTextField textField;
	private JLabel lblNewLabel_1;
	private JTextField textField_1;
	private JLabel label;
	private JTextField textField_2;
	private JLabel label_1;
	private JTextField textField_3;
	private JLabel label_2;
	private JTextField textField_4;
	private JLabel label_3;
	private JTextField textField_5;
	private JLabel label_4;
	private JTextField textField_6;
	private JButton btnNewButton;
	private JButton button;
	private JButton button_1;
	private JButton button_2;
	private JButton button_3;
	private JButton button_4;
	private JTable table;
	
	/**
	 * Create the frame.
	 */
	public TelaArma() {
		setResizable(false);
		setBounds(100, 100, 600, 600);
		
		panelCampos = new JPanel();
		getContentPane().add(panelCampos, BorderLayout.NORTH);
		panelCampos.setLayout(new GridLayout(0, 2, 5, 5));
		
		JLabel lblNewLabel = new JLabel("New label");
		panelCampos.add(lblNewLabel);
		
		textField = new JTextField();
		panelCampos.add(textField);
		textField.setColumns(10);
		
		lblNewLabel_1 = new JLabel("New label");
		panelCampos.add(lblNewLabel_1);
		
		textField_1 = new JTextField();
		textField_1.setColumns(10);
		panelCampos.add(textField_1);
		
		label = new JLabel("New label");
		panelCampos.add(label);
		
		textField_2 = new JTextField();
		textField_2.setColumns(10);
		panelCampos.add(textField_2);
		
		label_1 = new JLabel("New label");
		panelCampos.add(label_1);
		
		textField_3 = new JTextField();
		textField_3.setColumns(10);
		panelCampos.add(textField_3);
		
		label_2 = new JLabel("New label");
		panelCampos.add(label_2);
		
		textField_4 = new JTextField();
		textField_4.setColumns(10);
		panelCampos.add(textField_4);
		
		label_3 = new JLabel("New label");
		panelCampos.add(label_3);
		
		textField_5 = new JTextField();
		textField_5.setColumns(10);
		panelCampos.add(textField_5);
		
		label_4 = new JLabel("New label");
		panelCampos.add(label_4);
		
		textField_6 = new JTextField();
		textField_6.setColumns(10);
		panelCampos.add(textField_6);
		
		btnNewButton = new JButton("New button");
		panelCampos.add(btnNewButton);
		
		button = new JButton("New button");
		panelCampos.add(button);
		
		button_1 = new JButton("New button");
		panelCampos.add(button_1);
		
		button_2 = new JButton("New button");
		panelCampos.add(button_2);
		
		button_3 = new JButton("New button");
		panelCampos.add(button_3);
		
		button_4 = new JButton("New button");
		panelCampos.add(button_4);
		
		table = new JTable();
		getContentPane().add(table, BorderLayout.CENTER);

	}

}
